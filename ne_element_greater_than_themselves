class Solution{
    public static void main(String[] args){
        int count=0;
        int arr[]={-4,3,7,9,3,9,4};
        int max=arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
        for(int i=0;i<arr.length;i++){
            if(arr[i]<max){
                count++;
            }
        }
        System.out.println(count);
    }
}
