import java.lang.Math;
 
public class GreaterOfTwo {

    public static int greaterOfTwo(int x, int y) {

        return (Math.abs(x - y) + (x + y)) / 2;
    }
 
    
    public static void main(String[] args) {
        int a, b;
        a = 86;
        b = 34;
 
        System.out.println(greaterOfTwo(a, b));
    }
}
