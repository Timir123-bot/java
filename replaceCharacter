class replaceCharacter{
    public static void main(String[] args){
        String str="aabbbcc";
        char[] charArray=str.toCharArray();
        for(int i=0;i<charArray.length;i++){
            if(charArray[i] == 'a'){
                charArray[i] = 'b';
            }
           else if(charArray[i] == 'b'){
                charArray[i] = 'a';
            }
        }
        System.out.println(new String(charArray));
    }
}
