class power {
    
    public static boolean isPowerofTwo(int n) {
        return (n > 0) && ((n & (n - 1)) == 0);
    }

    public static void main(String[] args) {
        int n = 16;
        if (isPowerofTwo(n)) {
            System.out.println("Yes");
        } else {
            System.out.println("No");
        }
    }
}
